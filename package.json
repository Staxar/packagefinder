{
  "name": "packagefinder",
  "displayName": "Package Finder",
  "description": "A VS Code extension to search for and install npm or yarn packages.",
  "version": "0.0.1",
  "icon": "images/packagefinder.webp",
  "publisher": "Marcin Domagala",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "packagefinder.finder",
        "title": "Package Finder"
      }
    ],
    "keybindings": [
      {
        "command": "packagefinder.finder",
        "key": "ctrl+alt+i",
        "mac": "shift+cmd+i"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
